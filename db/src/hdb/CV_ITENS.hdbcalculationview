      <viewAttribute id="PESO_UNITARIO"/>
    </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="PESO_ITEM" datatype="DECIMAL" length="10" scale="2" expressionLanguage="SQL">
        <formula>&quot;PESO_UNITARIO&quot;*&quot;QUANTIDADE&quot;</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="PRECO_ITEM" datatype="DECIMAL" length="12" scale="2" expressionLanguage="SQL">
        <formula>&quot;PRECO_UNITARIO&quot;*&quot;QUANTIDADE&quot;</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="CUSTO_ITEM" datatype="DECIMAL" length="12" scale="2" expressionLanguage="SQL">
        <formula>&quot;CUSTO_UNITARIO&quot;*&quot;QUANTIDADE&quot;</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <input node="Join_1"/>
  </calculationView>
